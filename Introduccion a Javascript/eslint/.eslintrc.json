
 {
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },

    "rules": {
        "eqeqeq":"warn",
        "strict": "off",
        "quotes":["error", "double"],
        "semi":["error","always"],
        "no-dupe-keys": "error"
    }
  }